{
    "ado": {
        "serviceUrl": "https://dev.azure.com",
        "organization": "sjellis",
        "project": "sje-ado-labs",
        "queries": {
            "releases": {
                "index": "https://vsrm.dev.azure.com/{organization}/{project}/_apis/release/releases?api-version=6.0"
            },
            "repos": {
                "index": "https://dev.azure.com/{organization}/{project}/_apis/git/repositories?api-version=6.0&includeLinks=true",
                "item": "https://dev.azure.com/{organization}/{project}/_apis/git/repositories/{repositoryId}?api-version=6.1-preview.1"
            }
        }
    },
    "schema": {
        "version": "0.1.0"
    }
}